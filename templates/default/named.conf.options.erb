options {
  directory "<%= @cache_path %>";

<%= render_options(@options) %>
};

logging {
  channel default_log {
    file "<%= @log_file %>" versions 5 size 128M;
    print-time yes;
    print-severity yes;
    print-category yes;
  };
      
  category default { default_log; };
  category general { default_log; };
};
